<header id="header" class="bgm-indigo">
  <ul class="header-inner">
    <% if current_user %>
      <li id="menu-trigger" data-trigger="#sidebar">
        <div class="line-wrap">
          <div class="line top"></div>
          <div class="line center"></div>
          <div class="line bottom"></div>
        </div>
      </li>

    <% end %>
    <li class="logo hidden-xs">
      <a href="<%= current_user ? dashboard_path(current_user) : root_path %>">University of Colorado Cancer Center</a>
    </li>
  </ul>
</header>

